{
  "expo": {
    "name": "Neo Shopper",
    "slug": "neo-shopper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/neoshopper.png",
    "scheme": "neoshopper",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/neoshopper.png",
      "resizeMode": "contain",
      "backgroundColor": "#DFE5F2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chicakelitte.neoshopper"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "android": {
      "package": "com.chicakelitte.neoshopper",
      "versionCode": 1,
      "icon": "./assets/images/neoshopper.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2b8fa558-7076-4ea4-b85d-dee8a26d55c6"
      }
    }
  }
}
